@page "/general_error_page"

<div class="row-layout general-error-page-container">
    <div class="column-layout container">
        <img src="assets/goowid-logo.svg" class="image" />
        <img src="assets/error-robots 1.svg" class="image_1" />
        <div class="column-layout group">
            <h6 class="h6">
                Oops! Something went wrong...
            </h6>
            <p class="typography__body-1 text">
                We're having trouble processing your request right now. Please try again later.
            </p>
        </div>
        <IgbButton Variant="ButtonVariant.Outlined" class="button" general_error_page-scope>
            <span>Go to Homepage</span>
            <span class="material-icons">
                arrow_forward
            </span>
            <IgbRipple></IgbRipple>
        </IgbButton>
    </div>
</div>

